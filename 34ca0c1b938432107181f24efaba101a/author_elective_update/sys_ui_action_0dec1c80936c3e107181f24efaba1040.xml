        <name>Copy Feature</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[var helper = new EapItemsDeepCopy(current);

if (helper.showCopyAction(current)) {
    var result = helper.deepCopy(current);

    if (!gs.nil(result)) {
        var recordLink = '<a href="' + result.newRecordLink + '">' + GlideStringUtil.escapeHTML(current.getValue('number')) + '</a>';
        gs.addInfoMessage(gs.getMessage('Feature {0} copied successfully', recordLink));

        gs.setRedirect(result.newRecordLink + '&sysparm_returned_action=sysverb_copy_feature&sysparm_view=feature');

        if (action.getGlideURI().get('sysparm_modal_form') == 'true')
            action.setReturnURL('$snAgModal_close_dialog.do?sysparm_returned_action=sysverb_copy_feature');
        else
            action.setReturnURL(current);
    }
}]]></script>
